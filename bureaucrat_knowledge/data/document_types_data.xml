    <record id="bureaucrat_document_type_bmp"
            model="bureaucrat.document.type">
        <field name="name">Business Process Model</field>
        <field name="code">bmp</field>
        <field name="number_generator_id" ref="sequence_document_type"/>
    </record>
</odoo>
