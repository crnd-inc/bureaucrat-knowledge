    </record>
    <record id="bureaucrat_document_type_fs"
            model="bureaucrat.document.type">
        <field name="name">Act of change</field>
        <field name="code">fs</field>
        <field name="number_generator_id" ref="sequence_document_type"/>
    </record>
    <record id="bureaucrat_document_type_ts"
            model="bureaucrat.document.type">
        <field name="name">Functional specification</field>
        <field name="code">ts</field>
        <field name="number_generator_id" ref="sequence_document_type"/>
    </record>
    <record id="bureaucrat_document_type_sop"
            model="bureaucrat.document.type">
        <field name="name">Standard operating procedure</field>
        <field name="code">sop</field>
