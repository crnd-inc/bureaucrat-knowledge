        <field name="name">access for request wizard stop work</field>
        <field name="model_id" ref="bureaucrat_knowledge_rel_docs.model_document_wizard_manage_related_documents"/>
        <field name="domain_force">[
                ('create_uid', '=', user.id),
            ]</field>
