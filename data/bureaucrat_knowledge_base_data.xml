    <record id="reference_website_service_desk_modules"
            model="bureaucrat.knowledge.category">
        <field name="name">Website Service Desk Modules</field>
        <field name="parent_id"
               ref="bureaucrat_documentation_reference"/>
    </record>

    <record id="reference_generic_condition_modules"
            model="bureaucrat.knowledge.category">
        <field name="name">Generic Condition Modules</field>
        <field name="parent_id"
               ref="bureaucrat_documentation_reference"/>
    </record>

</odoo>
